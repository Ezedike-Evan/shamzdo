{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "android":{
      "artifact":"apk",
      "image":"latest",
      "env":{
        "JAVA_HOME":"/usr/lib/jvm/java-17-openjdk-amd64"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
